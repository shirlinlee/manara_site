/* Body */
body{
    font-family: Microsoft JhengHei;
}
hr{
    margin: 20px 0; 
}
hr.col-md-8{
  padding: 0px!important;
}
hr.mobile-show{
  display: none;
}
a{
  color: blue!important;
  text-decoration: underline!important;
}
a.btn{
    font-weight: bold;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
ul.mynavi_list.clearfix {
    font-size: 14px;
    font-weight: bold;
}
h1, h2, h3, h4, h5, h6{
    padding: 15px 0 10px 0;
    text-align: center;
    font-weight: bold;
}

/* section padding */
.sec-wrap,.sec-img-wrap{
	padding: 30px 0;
}
.sec-img-wrap #main_image{
  margin-top: 0px!important;
  margin-bottom: 0px!important;
}
/* padding & margin 
.pd-text-1, .pd-text-2{
  padding: 13px 0;
}*/
.pd-text-1 , .pd-text-2 span{
  font-size: 18px;
  font-weight: bold;
  padding: 0 1em 0 0!important;
}
.pd-20{
  padding: 20px 0px!important;
}
.pd-b-20{
  padding: 0 0 20px!important;
}
/* back-to-top */
#back-to-top {
    position: fixed;
    bottom: 43px;
    right: 0;
    z-index: 9999;
    width: 35px;
    height: 35px;
    background-color: #000;
    border: 0;
    text-decoration: none;
    transition: opacity 0.2s ease-out;
    opacity: 0;
}
#back-to-top:hover {
    background: #000;
    transition: 0.2s ease-out;
}
#back-to-top.show {
    opacity: 1;
}
/* ICON */
.qa-box{
  display: table;
  padding-bottom: 10px;
}
.qa-box-cell{
    display: table-cell;
    vertical-align: top;
}
.qa-box .qa-box-cell:last-child{
  vertical-align: middle;
}
.icon-q{
  content: url('../img/icon/Q_44x44.png');
  padding-top: 5px;
  padding-right: 10px; 

}
.icon-a{
  content: url('../img/icon/A_44x44.png');
  padding-top: 5px;
  padding-right: 10px; 
}

/*** fixed Section Start ***/
/* Header */
.navbar [class*=col-], .navbar-brand{
    padding: 0;
}
.navbar-header{
    float: none;
    /*white-space: nowrap;*/
}
.navbar-header .logo{
    margin: auto;
    width: 200px;
    height: 60px;
    padding-top: 4px;
}
.header-icon{
    position: absolute;
    right: 0;
    font-size: 0;
}
.header-icon div{
    display: inline-block;
}
/*.header-icon .icon-member{
    border-right: 1px solid black;    
}*/
.navbar-content{
    width: 100%;
    text-align: center;
    font-size: 0;
}
.navbar-nav{
    display: inline-block;
    float: none;
    font-size: 0;
}
.navbar-nav li{
    font-weight: bold;
    padding: 0px;
    font-size: 16px;
}
.border {
    border-top: 7px solid #cf0b2c;
    border-bottom: 3px solid #cf0b2c;
}
#top-nav hr{
    margin: 0;
    border:0; height:1px;
    background-color:#949494;
}
/* Link-infos */
.link-infos img{
  width: 100%;
}
.link-infos p{
  padding-top: 15px;
}

/* H2 title */
h2.title {
    margin-bottom: 10px;
    padding: 0px;
    /*background: url(../img/banner/banner0001.png);*/
    border-top: solid 1px #ebeced;
    height:40px;
    line-height:45px;
    padding-left:5px;
    color: #FCFCFC;
    /*background-color: #fef3d8;*/
    font-size: 170%;
}

/* Footer */
.footer-bg{
  background: #CF0B2C;
}
.footer-wrap{
    padding-top: 30px;
}
.doc-footer{
    border-top: 1px solid #d6d6d6;
}
.doc-footer [class*=columns-]>[class*=col-]{
    padding: 0;
}
.doc-footer hr{
    margin: 0;
    border:0; height:1px;
    background-color:#949494;
}
#footer_lists{
  display: inline-block;
  width: 48%;
}
.footer-maNara{
  width: 100%!important;
  padding-top: 5px;
  float: left;
}
#footer_lists ul:first-child{
    margin-right: 40px;
}
#footer_lists ul{
    display: inline-block;
    float: left;
    font-size: 17px;
}
#footer_lists ul li{
    padding: 5px 0;
}
#footer_lists a{
    color: black;
    font-weight: bold;
}
.footer_icon{
    display: inline-block;
    padding: 8px;
}
#footer_infos{
  padding-bottom: 30px;
}
#footer_infos ul{
    font-weight: bold;   
    font-size: 17px;
    padding: 0 0 10px; 
}
.footer-maNara{
  float: left;
  font-weight: bold;
  font-size: 17px;
  padding-bottom: 30px;
}
/*** fixed Section End ***/


/*** Index Page Start ***/
.product-sec{
    display: inline-block;
    text-align: center;
}
.product-series h4 ,.link-infos h4{
    background: #f67f85;
    color: white;
    text-align: left;
    padding: 5px 15px;
}
/* Youtube Video*/
.video-container {
  position: relative;
  padding-bottom: 56.25%;
  padding-top: 30px;
  margin: 30px 0;
  height: 0;
  overflow: hidden;
}
.video-container iframe,
.video-container object,
.video-container embed {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
/* social-media */
.social-media{
  margin: 30px 0;
}
/*** Index Page End ***/

/*** features Page Start ***/
.features div {
    text-align: center;
}
.features h4{
    color: white;
    background: #cf0b2c;
    margin: 35px auto 10px;
    padding: 5px 35px;
    display: table;
}
.features h4:first-child{
    margin: 0px auto 10px;
}
/*** features Page End ***/

/*** Product Page Start ***/
.kui-select {
    background: none;
}
.product-box{
  display: flex;
}
.info-border{
  height: inherit;
}
.product-pc-title{
  height: 20%;
}
.product-pc-title p{
    padding: 0px 0 15px;
    margin: 0; 
    font-weight: bold;
}
.product-content{
  height: 80%;
  padding: 20px 0;
  text-align: justify;
  text-justify:inter-ideograph;
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
}
.product-mb-title{
    display: none;
}
#product iframe{
  width: 100%;
}
.addToCart{

}
.product-input{
    white-space: nowrap;
    font-size: 0;
    height: 47px;
}
.product-input span{
    display: inline-block;
    width: 12%;
    padding: 6px 0;
    font-size: 16px;
}
.product-input .kui-select, .product-input .ctrl-input{
    display: inline-block;
    width: 88%;
    font-size: 16px;
}
.p-info{
  font-size: 20px!important;
  font-weight: bold;
}
.product-input .p-info {
    font-size: 20px!important;
    font-weight: bold;
    width: 100%;
    display: block;
}
.p-info-red{
  color: #dc2723;
  font-size: 22px!important;
  font-weight: bold;
}
.product-input .p-info-red {
    color: #dc2723;
    font-size: 22px !important;
    font-weight: bold;
    width: 100%;
    display: block;
}
.bt-color-red{
  background: #CF0B2C;
  font-size: 0;
}
.bt-color-green{
  background: #00962C;
  font-size: 0;
}
.add-button button{
  width: 100%;
}
.add-button span{
  font-size: 16px;
}
.add-button button:before{
  content: url(../img/icon/icon-cart.png);
  position: relative;
  right: 0;
  top: 7px;
  font-size: 0;
}
.add-button a{
    padding-bottom: 13px; 
    width: 100%;
}

/*** Product PAGE End ***/


/*** ShoppingCart Page Start ***/
.product-table{
    text-align: center;
    font-weight: bold;
}
.product-table th{
    background: #eaeaea;
    border: 1px solid grey;
    font-weight: bold;
}
.product-table tr td{
    padding: 10px 5px;
    border: 1px solid grey;
}
.product-table .tb-th{
    text-align: right;
    background: #eaeaea;
}
.product-mb{
    display: none;
    width: 100%;
}
.product-mb p{
  font-size: 16px;
}
.table-section{
    border: 1px solid #d6d6d6;
}
.product-mb-table tr{
    border: 1px solid grey;
}
.product-mb-table tr td{
    padding: 10px;
}
.product-mb-table .mb-pic{
    max-width: 100px;
        width: 150px;
}
.product-mb-table .mb-pic img{
        width: 100%;
}
.product-mb-table .mb-delete{
    width: 50px;
}
.product-title span{
  padding: 0 8px;
}
.product-title br{
  display: none;
}
.submit-button{
  font-size: 0;
}
.submit-button button {
    padding: 0 5px;
}
.submit-button li{
    display: inline-block;
    padding: 0 10px;
}
span.goBack:before{
  content:url('../img/icon/icon-goBack.png');
  position:relative;
  z-index:100000; 
  top: 3px;
  padding-right: 5px;
}
.submit-button button{
  width: 100%;
}
span.sub-order:after{
  content:url('../img/icon/icon-submit.png'); 
  position: relative;
  right: 0;
  top: 4px;
}
.color-grey{
    background: #aeaeae;
}
/*** ShoppingCart Page End ***/


/*** Chairman Page Start ***/
/*#main_image img{
    width: 100%;
}*/
.chairman img{
    width: 100%;
}
/*** Chairman Page End ***/


/*** bxSlider Start ***/
/* index slider */
.banner-slider .bx-wrapper {
    margin: 0;
}
.banner-slider .bx-wrapper .bx-pager{
    bottom: 35px;
}
.bx-wrapper .bx-pager.bx-default-pager a {
    background: #fff;
}
.bx-pager img{
    width: 25%;
}
/* product slider */
#product .bx-wrapper img {
    width: 100%;
    display: block;
    margin: auto;
}
.bx-viewport{
    height: 340px;
}
.mm-tabs-wrapper .tab-item{
    padding: 0 40px;
}
.bx-wrapper .bx-next {
    right: -25px;
}
.bx-wrapper .bx-next:hover {
    background-position: -43px 0;
}
.bx-wrapper .bx-prev {
    left: -25px;
}
.bx-wrapper .bx-prev:hover {
    background-position: 0 0;
}
#product .bx-wrapper .bx-pager{
  display: none;
}
.bx-wrapper .bx-controls-direction a{
  display: none;
}
#gallery .bx-wrapper .bx-controls-direction a{
  display: block;
  -webkit-transition: 0s ease;
  transition: 0s ease;
}
/*** bxSlider End ***/
.navbar-content a, .footer-wrap a ,.navbar-nav-item a, .link-infos a, .product-series a, a.btn{
    color: black!important;
    text-decoration: none!important;
}
a.navbar-toggle {
    color: black!important;
    text-decoration: none!important;
}
a:link:hover, a[href]:hover {
    color: none!important;
    text-decoration: underline!important;
}
.navbar-nav-item:hover .navbar-nav-link {
    color: none!important;
    background: none!important;
}
/*
.product-sec img {
    height: 222px;
}
*/
.align-right{
  font-size: 0;
}

/* social-media RWD */

