table.delivname td, table.delivname th {
    border: 1px solid #ccc!important;
}
input#zipCodeSelect {
    padding-bottom: 30px;
}
select#dialogZipCode {
    width: 100%!important;
}
#zipSearchDialog > div:first-child{
  font-size: 0;
}
#dialogPref, #dialogCity1, #dialogCity2 {
    width: 33%!important;
    font-size: 16px;
    margin-bottom: 20px;
}
#mypagecolumn{
  padding-left: 25px;
  padding-right: 25px;
 }

